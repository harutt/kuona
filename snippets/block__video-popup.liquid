{% comment %}
	Video Popup block
	Accepts:
	- block: block object
{% endcomment %}
{% liquid
	capture animation_attr
		render 'animation-attr', type: block.settings.animation_type
	endcapture
%}
<div
	class="gsp-block__{{ block.id }} gsp-block gsp-block-video-popup" {{- animation_attr -}} {{ block.shopify_attributes }}>
	<div class="gsp-block__inner">
		{% render 'el__video-popup', args: block, has_background: true, image_plackholder_index: image_plackholder_index %}
	</div>
</div>