{%- liquid
	assign layout = section.settings.layout
	assign content_align = section.settings.content_align
	assign image = section.settings.image
	assign media_width = section.settings.media_width
	assign media_height = section.settings.media_height
	assign media_height_mobile = section.settings.media_height_mobile
	assign media_gap = section.settings.media_gap
	assign media_gap_mobile = section.settings.media_gap_mobile

	capture animation_attr
		render 'animation-attr', type: section.settings.animation_type
	endcapture
-%}
{% render 'section-start', name: 'half-media', custom_class: "gsp-section-half-media" %}
<div
	class="gsp-half-media-inner row layout-{{ layout }} {%- if content_align != 'default' %} align-items-{{ content_align }} {% endif %}">
	<div
		class="gsp-half-media-banner col {%- if section.settings.hover_style != 'none' %} img-hover-{{ section.settings.hover_style }}{% endif %} {%- if section.settings.hover_effect != 'none' %} img-effect img-effect-{{ section.settings.hover_effect }}{% endif %} {{ section.settings.custom_class_media }}" {{- animation_attr -}}>
		{% render 'responsive-image',
			image: image,
			placeholder: "image"
		%}
	</div>
	<div class="gsp-half-media-content position-relative col {{ section.settings.custom_class_content }}">
		{% if section.settings.slider_enable %}
			{%- liquid
				assign item_show_xl = section.settings.item_show_xl
				assign item_show_lg = section.settings.item_show_lg
				assign item_show_md = section.settings.item_show_md
				assign item_show_sm = section.settings.item_show_sm
				assign item_show_xs = section.settings.item_show_xs
				assign classes_slider = 'gsp-slider slick-slider gsp-gutter-wrap'
				if section.settings.vertical_align != "default"
					assign classes_slider = classes_slider | append: " slick-align-items-" | append: section.settings.vertical_align
				endif
			-%}

			<div {% render 'slider-attr',
				classes: classes_slider,
				item_to_show: item_show_xl,
				arrows: section.settings.slider_show_navigation,
				dots: section.settings.slider_show_pagination,
				auto_height: section.settings.slider_auto_height,
				autoplay: section.settings.slider_autoplay,
				autoplay_speed: section.settings.slider_autoplay_timeout,
				fade: section.settings.slider_fade,
				loop: section.settings.slider_loop,
				center_mode: section.settings.slider_center_mode,
				center_padding: section.settings.slider_center_padding,
				lg_settings: true,
				item_to_show_on_lg: item_show_lg,
				item_to_scroll_on_lg: item_show_lg,
				md_settings: true,
				item_to_show_on_md: item_show_md,
				item_to_scroll_on_md: item_show_md,
				sm_settings: true,
				item_to_show_on_sm: item_show_sm,
				item_to_scroll_on_sm: item_show_sm,
				xs_settings: true,
				item_to_show_on_xs: item_show_xs,
				item_to_scroll_on_xs: item_show_xs
			%}
				{%- if section.settings.slider_navigation_after_heading and section.settings.slider_show_navigation -%}
					data-slick-append-arrows=".gsp-arrows-{{ section.id }}"
				{%- endif -%}
			>
				{% for block in section.blocks %}
					{% if block.type == 'layer_image' or block.type == 'layer_html' %}
						{% assign has_layer = true %}
					{%- endif -%}
					{% if block.type != 'layer_image' and block.type != 'layer_html' %}
						<div class="gsp-block__wrap {%- if block.settings.custom_class != blank %} {{ block.settings.custom_class }}{% endif %}">
					{%- endif -%}
					{% render 'block__render', block: block %}
					{% if block.type != 'layer_image' and block.type != 'layer_html' %}
						</div>
					{%- endif -%}
				{% endfor %}
			</div>
			{% if has_layer == true %}
				{% for block in section.blocks %}
					{% if block.type == 'layer_image' %}
						{% render 'block__layer-image', block: block %}
					{% endif %}
					{% if block.type == 'layer_html' %}
						{% render 'block__layer-html', block: block %}
					{% endif %}
				{% endfor %}
			{%- endif -%}
		{% else %}
			<div class="row gsp-gutter-wrap position-relative">
				{% for block in section.blocks %}
					{%- liquid
						assign col_xl = block.settings.col_xl
						assign col_lg = block.settings.col_lg
						assign col_md = block.settings.col_md
						assign col_sm = block.settings.col_sm
						assign col = block.settings.col_xs
						assign class_item = 'gsp-block__wrap col'
						if block.settings.custom_class != blank
							assign class_item = class_item | append: " " | append: block.settings.custom_class
						endif
						if forloop.first
							assign class_item = class_item | append: " gsp-block__is-first"
						endif
						if forloop.last
							assign class_item = class_item | append: " gsp-block__is-last"
						endif
						if block.type == 'offset_block'
							assign class_item = class_item | append: " my-0"
							if block.settings.hide_on_xs
								assign class_item = class_item | append: " d-none"
							endif
							if block.settings.hide_on_sm
								assign class_item = class_item | append: " d-sm-none"
							else
								assign class_item = class_item | append: " d-sm-block"
							endif
							if block.settings.hide_on_md
								assign class_item = class_item | append: " d-md-none"
							else
								assign class_item = class_item | append: " d-md-block"
							endif
							if block.settings.hide_on_lg
								assign class_item = class_item | append: " d-lg-none"
							else
								assign class_item = class_item | append: " d-lg-block"
							endif
							if block.settings.hide_on_xl
								assign class_item = class_item | append: " d-xl-none"
							else
								assign class_item = class_item | append: " d-xl-block"
							endif
						endif
					-%}
					{% if block.type != 'layer_image' and block.type != 'layer_html' %}
						<div {% render 'classes-col-bootstrap', additional: class_item, convert_class: false, col_xl: col_xl, col_lg: col_lg, col_md: col_md, col_sm: col_sm, col: col %}>
							{% render 'block__render', block: block %}
						</div>
					{% endif %}
					{% if block.type == 'layer_image' %}
						{% render 'block__layer-image', block: block %}
					{% endif %}
					{% if block.type == 'layer_html' %}
						{% render 'block__layer-html', block: block %}
					{% endif %}
				{% endfor %}
			</div>
		{% endif %}
	</div>
</div>
{% style %}
	{% if media_height != blank and media_height != -1 %}
		.gsp-section-{{ section.id }} .gsp-half-media-banner [style*=--aspect-ratio]:before{
		padding-top: {{ media_height }}px !important;
		}
	{%- endif %}
	{% if media_height_mobile != blank and media_height_mobile != -1 %}
		@media only screen and (max-width: 991px) {
		.gsp-section-{{ section.id }} .gsp-half-media-banner [style*=--aspect-ratio]:before{
		padding-top: {{ media_height_mobile }}px !important;
		}
		}
	{%- endif %}
	.gsp-section-{{ section.id }} {
	{%- if media_width != blank and media_width != -1 %}
		--gsp-half-media-width: {{ media_width }}%;
	{% endif -%}
	{%- if media_gap != blank and media_gap != -1 %}
		--gsp-half-media-gap: {{ media_gap }}px;
	{% endif -%}
	{%- if media_gap_mobile != blank and media_gap_mobile != -1 %}
		--gsp-half-media-gap-mobile: {{ media_gap_mobile }}px;
	{% endif -%}
	}
{% endstyle %}
{% render 'section-end' %}
