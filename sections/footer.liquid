{% render 'section-start', name: 'footer', has_header: false %}
<div class="gsp-footer row gsp-gutter-wrap {% if section.settings.vertical_align != "default" %} align-items-{{ section.settings.vertical_align }}{% endif %}">
	{% for block in section.blocks %}
		{%- liquid
			assign col_xl = block.settings.col_xl
			assign col_lg = block.settings.col_lg
			assign col_md = block.settings.col_md
			assign col_sm = block.settings.col_sm
			assign col = block.settings.col_xs
			assign class_item = 'gsp-block__wrap'
			if block.settings.custom_class != blank
				assign class_item = class_item | append: " " | append: block.settings.custom_class
			endif
			capture animation_attr
				render 'animation-attr', type: block.settings.animation_type
			endcapture
			if block.type == 'offset_block'
				assign class_item = class_item | append: " my-0"
				if block.settings.hide_on_xs
					assign class_item = class_item | append: " d-none"
				endif
				if block.settings.hide_on_sm
					assign class_item = class_item | append: " d-sm-none"
				else
					assign class_item = class_item | append: " d-sm-block"
				endif
				if block.settings.hide_on_md
					assign class_item = class_item | append: " d-md-none"
				else
					assign class_item = class_item | append: " d-md-block"
				endif
				if block.settings.hide_on_lg
					assign class_item = class_item | append: " d-lg-none"
				else
					assign class_item = class_item | append: " d-lg-block"
				endif
				if block.settings.hide_on_xl
					assign class_item = class_item | append: " d-xl-none"
				else
					assign class_item = class_item | append: " d-xl-block"
				endif
			endif
		-%}
		<div {% render 'classes-col-bootstrap', additional: class_item, convert_class: false, col_xl: col_xl, col_lg: col_lg, col_md: col_md, col_sm: col_sm, col: col %} {{- animation_attr -}}>
			{% render 'block__render', block: block %}
		</div>
	{% endfor %}
</div>
{% render 'section-end', has_header: false %}
