{%- liquid
	assign layout = section.settings.layout_image
	assign hide_image_mobile = section.settings.hide_image_on_mobile
	assign content_align = section.settings.content_align
	assign image = section.settings.image
	assign mw_popup = section.settings.mw_popup
	assign mw_popup_mobile = section.settings.mw_popup_mobile
	assign radius_wrap = section.settings.radius_wrap
	assign show_only_homepage = section.settings.show_only_homepage
	assign bg_color = section.settings.section_bg_color
	assign aspect_ratio = '4/5'

	assign inner_class = 'gsp-popup-promotion-inner d-flex'
	if layout == 'image-left'
		assign inner_class = inner_class | append: ' flex-row'
	endif
	if layout == 'image-right'
		assign inner_class = inner_class | append: ' flex-row-reverse'
	endif
	if layout == 'image-top'
		assign inner_class = inner_class | append: ' flex-column'
		assign aspect_ratio = '21/9'
	endif
	if layout == 'image-bottom'
		assign inner_class = inner_class | append: ' flex-column-reverse'
		assign aspect_ratio = '21/9'
	endif

	assign allow_popup = true
	if show_only_homepage and template != 'index'
		assign allow_popup = false
	endif
	if request.design_mode
		assign allow_popup = true
	endif
-%}
{%- if allow_popup %}
	<gsp-popup-promotion
	id="gsp-popup-promotion" class="gsp-popup-promotion layout-{{ layout }} gsp-section-{{ section.id }} modal fade {%- if hide_image_mobile %} hide-image-on-mobile {% endif %}" aria-label="Promotion Popup">
	<div class="modal-dialog modal-dialog-centered">
		<div class="modal-content position-relative overflow-hidden">
			<div
				class="{{ inner_class }} {%- if content_align != 'default' %} align-items-{{ content_align }} {% endif %}">
				{%- if layout != 'image-hide' %}
					<div
						class="gsp-popup-promotion-banner {%- if hide_image_mobile %} d-none d-lg-block {% endif %} {%- if section.settings.hover_style != 'none' %} img-hover-{{ section.settings.hover_style }}{% endif %} {%- if section.settings.hover_effect != 'none' %} img-effect img-effect-{{ section.settings.hover_effect }}{% endif %}">
						{% render 'responsive-image',
							image: image,
							placeholder: "image",
							aspect_ratio: aspect_ratio
						%}
					</div>
				{% endif %}
				<div class="gsp-popup-promotion-content">
					<button type="button" class="btn-close position-absolute z-index-15" data-bs-dismiss="modal"
							aria-label="Close"></button>
					<div class="row gsp-gutter-wrap position-relative">
						{% for block in section.blocks %}
							{%- liquid
								assign col_xl = block.settings.col_xl
								assign col_lg = block.settings.col_lg
								assign col_md = block.settings.col_md
								assign col_sm = block.settings.col_sm
								assign col = block.settings.col_xs
								assign class_item = 'gsp-block__wrap col'
								if block.settings.custom_class != blank
									assign class_item = class_item | append: " " | append: block.settings.custom_class
								endif
								if forloop.first
									assign class_item = class_item | append: " gsp-block__is-first"
								endif
								if forloop.last
									assign class_item = class_item | append: " gsp-block__is-last"
								endif
							-%}
							<div {% render 'classes-col-bootstrap', additional: class_item, convert_class: false, col_xl: col_xl, col_lg: col_lg, col_md: col_md, col_sm: col_sm, col: col %}>
								{% render 'block__render', block: block %}
							</div>
						{% endfor %}
					</div>
				</div>
			</div>
		</div>
	</div>
	{% style %}
		.gsp-popup-promotion.gsp-section-{{ section.id }} {
		{%- if mw_popup != blank and mw_popup != -1 %}
			--gsp-popup-promotion-mw: {{ mw_popup }}px;
		{% endif -%}
		{%- if mw_popup_mobile != blank and mw_popup_mobile != -1 %}
			--gsp-popup-promotion-mw-mobile: {{ mw_popup_mobile }}px;
		{% endif -%}
		{%- if radius_wrap != blank and radius_wrap != -1 %}
			--gsp-popup-promotion-radius: {{ radius_wrap }}px;
		{% endif -%}

		{% if bg_color != blank and bg_color != "rgba(0,0,0,0)" %}
			--gsp-section-bg-color: {{ bg_color }};
		{% endif %}
		{% if section.settings.section_bg_image != blank %}
			--gsp-section-bg-image: url({{ section.settings.section_bg_image | image_url }});
		{% endif %}
		{% if section.settings.section_bg_position != blank and section.settings.section_bg_position != "default" %}
			--gsp-section-bg-position: {{ section.settings.section_bg_position }};
		{% endif %}
		{% if section.settings.section_bg_size != blank and section.settings.section_bg_size != "default" %}
			--gsp-section-bg-size: {{ section.settings.section_bg_size }};
		{% endif %}
		}
	{% endstyle %}
	</gsp-popup-promotion>
{% endif %}
