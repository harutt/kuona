{% liquid
	capture class_container
		render 'classes-container', container: section.settings.container
	endcapture
	assign class_container = class_container | strip_newlines
	assign enable_search_mobile = section.settings.mobile_show_search
	assign enable_search = section.settings.show_search
	assign placeholder = 'search.placeholder_search_on_header' | t
	assign class_transparent_on_top = ""
	if section.settings.header_transparent_on_top
		assign class_transparent_on_top = "gsp-header-transparent"
	endif
	if section.settings.header_transparent_on_top_home and template == 'index'
		assign class_transparent_on_top = "gsp-header-transparent"
	endif

	assign logo_search = section.settings.logo
	if section.settings.logo_sticky != blank
		assign logo_search = section.settings.logo_sticky
	endif
%}
<header
	class="header layout-{{ section.settings.header_layout }} start-0 end-0 z-index-10 {{ class_transparent_on_top }} {{ section.settings.custom_class }}">
	{% render 'header__topbar', container: class_container %}
	{% render 'header__mobile', container: class_container %}
	{% render 'header__desktop', container: class_container %}
</header>
{% if enable_search_mobile or enable_search %}
	<div id="offcanvasSearch" data-gsp-append="body"
		 class="offcanvas offcanvas-end gsp-container-search gsp-search-offcanvas-end d-none"
		 tabindex="-1">
		<div class="offcanvas-header pb-0">
			<h3 class="h4">{{ 'header.search' | t }}</h3>
			<button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="{{ 'general.close' | t }}"></button>
		</div>
		<div class="offcanvas-body gsp-scrollbar">
			{% render 'el__search',
				id: 'gsp-search-popup-header',
				predictive_search: settings.search_enable_predictive,
				placeholder: placeholder,
				recommend: true,
				reset: false
			%}

		</div>
	</div>
{% endif %}
<script type="application/ld+json">
	{
	  "@context": "http://schema.org",
	  "@type": "Organization",
	  "name": {{ shop.name | json }},
	{% if settings.logo %}
      "logo": {{ settings.logo | image_url: width: 500 | prepend: "https:" | json }},
    {% endif %}
	"sameAs": [
	{{ settings.social_link_instagram | json }}
	{{ settings.social_link_twitter | json }},
	{{ settings.social_link_facebook | json }},
	{{ settings.social_link_pinterest | json }},
	{{ settings.social_link_snapchat | json }},
	{{ settings.social_link_tumblr | json }},
	{{ settings.social_link_youtube | json }},
	{{ settings.social_link_vimeo | json }},
	{{ settings.social_link_tiktok | json }},
	{{ settings.social_link_discord | json }},
	{{ settings.social_link_whatsapp | json }},
	{{ settings.social_link_spotify | json }},
	{{ settings.social_link_linkedin | json }},
	{{ settings.social_link_line | json }}
	],
	"url": {{ request.origin | append: page.url | json }}
	}
</script>

{%- if request.page_type == 'index' -%}
	{% assign potential_action_target = request.origin | append: routes.search_url | append: '?q={search_term_string}' %}
	<script type="application/ld+json">
		{
		  "@context": "http://schema.org",
		  "@type": "WebSite",
		  "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ request.origin | append: page.url | json }}
		}
	</script>
{%- endif -%}
