{% render 'section-start', name: 'banner' %}
{% if section.settings.slider_enable %}
	{%- liquid
		assign item_show_xl = section.settings.item_show_xl
		assign item_show_lg = section.settings.item_show_lg
		assign item_show_md = section.settings.item_show_md
		assign item_show_sm = section.settings.item_show_sm
		assign item_show_xs = section.settings.item_show_xs
	-%}

	<div {% render 'slider-attr',
		classes: 'gsp-slider slick-slider gsp-gutter-wrap',
		item_to_show: item_show_xl,
		arrows: section.settings.slider_show_navigation,
		dots: section.settings.slider_show_pagination,
		auto_height: section.settings.slider_auto_height,
		autoplay: section.settings.slider_autoplay,
		autoplay_speed: section.settings.slider_autoplay_timeout,
		fade: section.settings.slider_fade,
		loop: section.settings.slider_loop,
		center_mode: section.settings.slider_center_mode,
		center_padding: section.settings.slider_center_padding,
		lg_settings: true,
		item_to_show_on_lg: item_show_lg,
		item_to_scroll_on_lg: item_show_lg,
		md_settings: true,
		item_to_show_on_md: item_show_md,
		item_to_scroll_on_md: item_show_md,
		sm_settings: true,
		item_to_show_on_sm: item_show_sm,
		item_to_scroll_on_sm: item_show_sm,
		xs_settings: true,
		item_to_show_on_xs: item_show_xs,
		item_to_scroll_on_xs: item_show_xs
	%}
		{%- if section.settings.slider_navigation_after_heading and section.settings.slider_show_navigation -%}
			data-slick-append-arrows=".gsp-arrows-{{ section.id }}"
		{%- endif -%}
	>
		{% for block in section.blocks %}
			{% liquid
				capture animation_attr
					render 'animation-attr', type: block.settings.animation_type
				endcapture
				if block.settings.image == blank
					capture current
						cycle 1, 2, 3
					endcapture
				endif
			%}
			<div
				class="gsp-block__wrap {%- if block.settings.custom_class != blank %} {{ block.settings.custom_class }}{% endif %}">
				<div class="gsp-block__{{ block.id }} gsp-block gsp-block-banner" {{- animation_attr -}} {{ block.shopify_attributes }}>
					<div class="gsp-block__inner">
						{% render 'el__banner', args: block , image_plackholder_index: current %}
					</div>
				</div>
			</div>
		{% endfor %}
	</div>
{% else %}
	<div class="row gsp-gutter-wrap">
		{% for block in section.blocks %}
			{%- liquid
				assign col_xl = block.settings.col_xl
				assign col_lg = block.settings.col_lg
				assign col_md = block.settings.col_md
				assign col_sm = block.settings.col_sm
				assign col = block.settings.col_xs
				assign class_item = 'gsp-block__wrap col'
				if block.settings.custom_class != blank
					assign class_item = class_item | append: " " | append: block.settings.custom_class
				endif
				if forloop.first
					assign class_item = class_item | append: " gsp-block__is-first"
				endif
				if forloop.last
					assign class_item = class_item | append: " gsp-block__is-last"
				endif
				capture animation_attr
					render 'animation-attr', type: block.settings.animation_type
				endcapture
				if block.settings.image == blank
					capture current
						cycle 1, 2, 3
					endcapture
				endif
			-%}
			<div {% render 'classes-col-bootstrap', additional: class_item, convert_class: false, col_xl: col_xl, col_lg: col_lg, col_md: col_md, col_sm: col_sm, col: col %}>
				<div class="gsp-block__{{ block.id }} gsp-block gsp-block-banner" {{- animation_attr -}} {{ block.shopify_attributes }}>
					<div class="gsp-block__inner">
						{% render 'el__banner', args: block , image_plackholder_index: current %}
					</div>
				</div>
			</div>
		{% endfor %}
	</div>
{% endif %}
{% render 'section-end' %}
