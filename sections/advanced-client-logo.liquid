{% render 'section-start', name: 'advanced-client-logo' %}
{% liquid
	capture animation_attr
		render 'animation-attr', type: section.settings.animation_type
	endcapture

	assign id_logo_wrap = 'advanced-client-logo-' | append: section.id
	assign id_content_wrap = 'advanced-client-logo-content-' | append: section.id
	assign orther_setting_slider_content = '"focusOnSelect":true,"asNavFor":"#' | append: id_logo_wrap | append: '"'
	assign orther_setting_slider_logo = '"focusOnSelect":true,"asNavFor":"#' | append: id_content_wrap | append: '"'
	assign class_id = '.gsp-section-' | append: section.id
	assign align = section.settings.cl_align
	assign col_xl = section.settings.item_show_xl
	assign col_lg = section.settings.item_show_lg
	assign col_md = section.settings.item_show_md
	assign col_sm = section.settings.item_show_sm
	assign col = section.settings.item_show_xs

	assign content_color = section.settings.cl_content_color
	assign content_fs = section.settings.cl_content_fs
	assign content_mw = section.settings.cl_content_mw
	assign content_sp = section.settings.cl_content_sp
%}
{%- capture extended_css -%}
	{{ class_id }} .gsp-slider-advanced-client-logo-content {
		{%- if content_color != blank and content_color != "rgba(0,0,0,0)" %}
			--gsp-client-logo-content-color: {{ content_color }} !important;
		{% endif -%}
		{%- if content_fs != blank and content_fs != 0 %}
			--gsp-client-logo-content-fs: {{ content_fs }}px !important;
		{% endif -%}
		{%- if content_mw != blank and content_mw != 0 %}
			--gsp-client-logo-content-mw: {{ content_mw }}px !important;
		{% endif -%}
		{%- if content_sp != blank and content_sp != 0 %}
			--gsp-client-logo-content-sp: {{ content_sp }}px !important;
		{% endif -%}
	}
	{%- if align != blank %}
		{{ class_id }} .gsp-slider-advanced-client-logo .slick-track{
		-webkit-box-align: {{ align }};
		-ms-flex-align: {{ align }};
		align-items: {{ align }};
		}
		{{ class_id }} .gsp-slider-advanced-client-logo .slick-track {
		display: -webkit-box;
		display: -ms-flexbox;
		display: flex;
		}
	{% endif -%}
{%- endcapture -%}
<div id="{{ id_content_wrap }}" {% render 'slider-attr',
	classes: 'gsp-slider slick-slider gsp-slider-advanced-client-logo-content text-center',
	item_to_show: 1,
	arrows: false,
	dots: false,
	auto_height: section.settings.slider_autoplay,
	autoplay: section.settings.slider_autoplay,
	autoplay_speed: section.settings.slider_autoplay_timeout,
	fade: section.settings.slider_fade,
	loop: section.settings.slider_loop,
	orther_setting: orther_setting_slider_content
%}>
	{% for block in section.blocks %}
		<div
			class="gsp-block__wrap {%- if block.settings.custom_class != blank %} {{ block.settings.custom_class }}{% endif %}">
			<div
				class="gsp-block__{{ block.id }} gsp-block gsp-block-client-logo" {{- animation_attr -}}>
				<div class="gsp-block__inner">
					<h3 class="gsp-advanced-client-logo-content d-inline-block">
					{% if block.settings.client_logo_link != blank %}
						<a href="{{ block.settings.client_logo_link }}" class="d-block" title="client logo">
							{% endif %}
							{{ block.settings.content | newline_to_br }}
							{% if block.settings.client_logo_link != blank %}
						</a>
						{% endif %}
					</h3>
				</div>
			</div>
		</div>
	{% endfor %}
</div>
<div id="{{ id_logo_wrap }}" {% render 'slider-attr',
	classes: 'gsp-slider slick-slider gsp-gutter-wrap gsp-slider-advanced-client-logo',
	item_to_show: col_xl,
	arrows: section.settings.slider_show_navigation,
	dots: section.settings.slider_show_pagination,
	auto_height: section.settings.slider_auto_height,
	autoplay: section.settings.slider_autoplay,
	autoplay_speed: section.settings.slider_autoplay_timeout,
	fade: section.settings.slider_fade,
	loop: section.settings.slider_loop,
	center_mode: section.settings.slider_center_mode,
	center_padding: section.settings.slider_center_padding,
	lg_settings: true,
	item_to_show_on_lg: col_lg,
	item_to_scroll_on_lg: col_lg,
	md_settings: true,
	item_to_show_on_md: col_md,
	item_to_scroll_on_md: col_md,
	sm_settings: true,
	item_to_show_on_sm: col_sm,
	item_to_scroll_on_sm: col_sm,
	xs_settings: true,
	item_to_show_on_xs: col,
	item_to_scroll_on_xs: col,
	orther_setting: orther_setting_slider_logo
%}
	{%- if section.settings.slider_navigation_after_heading and section.settings.slider_show_navigation -%}
		data-slick-append-arrows=".gsp-arrows-{{ section.id }}"
	{%- endif -%}
>
	{% for block in section.blocks %}
		<div
			class="gsp-block__wrap {%- if block.settings.custom_class != blank %} {{ block.settings.custom_class }}{% endif %}">
			<div
				class="gsp-block__{{ block.id }} gsp-block gsp-block-client-logo" {{- animation_attr -}} {{ block.shopify_attributes }}>
				<div class="gsp-block__inner">
					{% render 'el__client-logo', args: block, args_style: section %}
				</div>
			</div>
		</div>
	{% endfor %}
</div>
{% render 'section-end', extended_css: extended_css %}
