{% liquid
	assign blog = blogs[section.settings.blog]
	assign skin_blogs = section.settings.bc_skin
	assign col_xl = section.settings.item_show_xl
	assign col_lg = section.settings.item_show_lg
	assign col_md = section.settings.item_show_md
	assign col_sm = section.settings.item_show_sm
	assign col = section.settings.item_show_xs
	assign blog_limit = section.settings.blogs_per_page
	assign enable_slider = section.settings.slider_enable

	if skin_blogs == nil or skin_blogs == 'default' or skin_blogs == 'inherit'
		assign skin_blogs = settings.bc_skin
	endif
	assign hover_style = nil
	if section.settings.bc_hover_style != 'default'
		assign hover_style = section.settings.bc_hover_style
	endif

	assign extended_css_item = 'gsp-listing-item'
	if section.settings.extended_css_item != nil and section.settings.extended_css_item != ''
		assign extended_css_item = extended_css_item | append: " " | append: section.settings.extended_css_item
	endif
	assign class_id = '.gsp-section-' | append: section.id
	assign layout = skin_blogs | remove_first: 'skin-' | split: '-'
	assign layout = layout[0]
	if layout == 'list' or layout == 'standard'
		assign is_list = true
	endif

	assign image_ratio = section.settings.bc_aspect_ratio
	assign image_size = section.settings.bc_image_size

	assign animation_type = section.settings.animation_type
	if animation_type == 'inherit'
		assign animation_type = settings.animation_type
	endif
%}
{%- capture blog_empity -%}
	{% for i in (1..blog_limit) %}
		{% liquid
			capture current
				cycle 1, 2
			endcapture
			assign placeholder = 'lifestyle-' | append: current
		%}
		{% if enable_slider != true %}
			<div {% render 'classes-col-bootstrap', additional: extended_css_item, col_xl: col_xl, col_lg: col_lg, col_md: col_md, col_sm: col_sm, col: col %}>
		{% endif -%}
		{% render 'article-skins__item',
			skin: skin_blogs,
			hover_style: hover_style,
			image_ratio: image_ratio,
			image_size: image_size,
			animation_type: animation_type,
			placeholder: placeholder
		%}
		{% if enable_slider != true %}
			</div>
		{% endif -%}
	{% endfor %}
{%- endcapture -%}
{%- capture extended_css -%}
	{% render 'article-card__style',
		class_id: class_id,
		render_string: true,
		title_color: section.settings.bc_title_color,
		title_mt: section.settings.bc_title_mt,
		excerpt_color: section.settings.bc_excerpt_color,
		excerpt_mt: section.settings.bc_excerpt_mt,
		meta_color: section.settings.bc_meta_color,
		meta_mt: section.settings.bc_meta_mt,
		img_spacing: section.settings.bc_image_spacing
	%}
{%- endcapture -%}
{% render 'section-start', name: 'blog-post', custom_class: "gsp-section-listing gsp-section-blog-post" %}
<div
	class="gsp-listing gsp-blogs-listing gsp-listing-layout-{{ layout }} gsp-blogs-listing-layout-{{ layout }} {%- if animation_type != 'none' %}has-animation{% endif -%}"
	data-section-id="{{ section.id }}"
	data-listing-id="{{ section.id }}"
>
	{% if enable_slider %}
		<div {% render 'slider-attr',
			is_list: is_list,
			classes: 'gsp-slider slick-slider gsp-gutter-wrap gsp-listing-inner',
			item_to_show: col_xl,
			arrows: section.settings.slider_show_navigation,
			dots: section.settings.slider_show_pagination,
			auto_height: section.settings.slider_auto_height,
			autoplay: section.settings.slider_autoplay,
			autoplay_speed: section.settings.slider_autoplay_timeout,
			fade: section.settings.slider_fade,
			loop: section.settings.slider_loop,
			center_mode: section.settings.slider_center_mode,
			center_padding: section.settings.slider_center_padding,
			lg_settings: true,
			item_to_show_on_lg: col_lg,
			item_to_scroll_on_lg: col_lg,
			md_settings: true,
			item_to_show_on_md: col_md,
			item_to_scroll_on_md: col_md,
			sm_settings: true,
			item_to_show_on_sm: col_sm,
			item_to_scroll_on_sm: col_sm,
			xs_settings: true,
			item_to_show_on_xs: col,
			item_to_scroll_on_xs: col
		%}
			{%- if section.settings.slider_navigation_after_heading and section.settings.slider_show_navigation -%}
				data-slick-append-arrows=".gsp-arrows-{{ section.id }}"
			{%- endif -%}
		>
			{% if blog != blank and blog.articles.size > 0 %}
				{%- for article in blog.articles limit: blog_limit -%}
					{% liquid
						capture current
							cycle 1, 2
						endcapture
						assign placeholder = 'lifestyle-' | append: current
					%}
					{% render 'article-skins__item',
						article: article,
						skin: skin_blogs,
						hover_style: hover_style,
						image_ratio: image_ratio,
						image_size: image_size,
						animation_type: animation_type,
						placeholder: placeholder
					%}
				{% endfor %}
			{% else %}
				{{ blog_empity }}
			{%- endif -%}
		</div>
	{% else %}
		{% if blog != blank and blog.articles.size > 0 %}
			{%- paginate blog.articles by blog_limit -%}
				<div class="gsp-listing-wrapper" data-gsp-listing-render>
					<div class="row gsp-gutter-wrap gsp-listing-inner">
						{%- for article in blog.articles -%}
							{% liquid
								capture current
									cycle 1, 2
								endcapture
								assign placeholder = 'lifestyle-' | append: current
							%}
							<div {% render 'classes-col-bootstrap', additional: extended_css_item, col_xl: col_xl, col_lg: col_lg, col_md: col_md, col_sm: col_sm, col: col %}>
								{% render 'article-skins__item',
									article: article,
									skin: skin_blogs,
									hover_style: hover_style,
									image_ratio: image_ratio,
									image_size: image_size,
									animation_type: animation_type,
									placeholder: placeholder
								%}
							</div>
						{% endfor %}
					</div>
					{% if paginate.pages > 1 and section.settings.paginate_type != "none" %}
						{% render 'el__pagination', paginate: paginate, type: section.settings.paginate_type, customClass: 'mt-5' %}
					{%- endif -%}
				</div>
			{%- endpaginate -%}
		{% else %}
			<div class="gsp-listing-wrapper" data-gsp-listing-render>
				<div class="row gsp-gutter-wrap gsp-listing-inner">
					{{ blog_empity }}
				</div>
			</div>
		{%- endif -%}
	{%- endif -%}
</div>
{% render 'section-end', has_header: false, extended_css: extended_css %}
